    "def like (parameters):\n",
    "    m = parameters[0]; \n",
    "    b = parameters[1]; \n",
    "    sigma = parameters[2]\n",
    "    for i in range(len(df.x)):\n",
    "        y_hat = m * df.x + b\n",
    "    MLE_norm = -np.sum(norm.logpdf(df.y, loc=y_hat, scale = sigma))\n",
    "    return MLE_norm\n",
    "\n",
    "like_model = minimize(like, np.array([1,1,1]), method='Nelder-Mead')\n",
    "\n",
    "display(like_model)\n",
    "\n",
    "fig = plt.subplots(figsize=(10,8));\n",
    "plt.scatter(df.x,df.y, alpha=0.3);\n",
